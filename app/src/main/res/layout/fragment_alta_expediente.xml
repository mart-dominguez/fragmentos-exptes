<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".AltaExpediente">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Alta de expediente" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
    android:text="@string/expte_nro" />
    <EditText
        android:id="@+id/expteNro"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/expte_inic" />
    <EditText
        android:id="@+id/expteIniciador"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/expte_tema" />
    <EditText
        android:id="@+id/expteTema"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:text="Guardar"
        android:id="@+id/btnGuardar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</LinearLayout>